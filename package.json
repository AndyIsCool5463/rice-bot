{
    "name": "ricebot",
    "version": "1.0.0",
    "main": "./dist/index.js",
    "license": "MPL-2.0",
    "repository": "https://github.com/longshotdev/rice-deployment.git",
    "private": true,
    "scripts": {
        "tasks": "node cli/display.js",
        "dev": "nodemon index.ts",
        "test": "ava-ts --timeout=2m",
        "test:watch": "ava-ts --timeout=2m --watch",
        "build": "env NODE_ENV=production tsc",
        "start": "env NODE_ENV=production NODE_PATH=./dist node ./dist/index --unhandled-rejections=strict"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {
        "@discordjs/opus": "^0.2.1",
        "ascii-table": "^0.0.9",
        "ava": "^3.8.2",
        "ava-ts": "^0.25.2",
        "cachegoose": "^8.0.0",
        "chalk": "^4.0.0",
        "cross-env": "^7.0.2",
        "discord.js": "^12.1.1",
        "dotenv": "^8.2.0",
        "fs-nextra": "^0.5.1",
        "moment": "^2.27.0",
        "mongodb": "^3.5.6",
        "mongoose": "^5.9.10",
        "node-fetch": "^2.6.0",
        "nodemon": "^2.0.3",
        "ts-node": "^8.8.2",
        "typescript": "^3.9.6"
    },
    "devDependencies": {
        "@ava/typescript": "^1.1.1",
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-typescript": "^7.9.0",
        "@babel/register": "^7.9.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.6",
        "@types/express-session": "^1.17.0",
        "@types/jest": "^26.0.3",
        "@types/lodash": "^4.14.150",
        "@types/mongodb": "^3.5.25",
        "@types/mongoose": "^5.7.12",
        "@types/node": "^13.11.1",
        "@types/node-fetch": "^2.5.7",
        "@types/passport": "^1.0.3",
        "@types/ws": "^7.2.6",
        "@typescript-eslint/eslint-plugin": "^3.4.0",
        "@typescript-eslint/parser": "^3.4.0",
        "babel-eslint": "^10.1.0",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "eslint": "^7.3.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^25.5.4",
        "lodash": "^4.17.19",
        "node-pre-gyp": "^0.15.0",
        "ts-jest": "^25.4.0"
    },
    "ava": {
        "files": [
            "tests/**/*.ts"
        ],
        "typescript": {
            "extensions": [
                "ts"
            ]
        }
    }
}
